  <ownedViewpoints endUserDocumentation="This viewpoint provides the &quot;BOMI Detailed Diagram&quot;, which displays all BOMI details." name="BOMI Details" modelFileExtension="bomi">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" endUserDocumentation="This kind of diagram displays all BOMI details, like the particular attributes of the elements." name="BOMI Detailed Diagram" domainClass="bomi::BOMIModel" enablePopupBars="true">
      <metamodel href="../../se.gu.cse.idxse.bomi/model/bomi.ecore#/"/>
      <validationSet>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BONoLowModularity" level="WARNING" message="The boundary object has a low modularity." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:self.modularity &lt;> bomi::HighLow::Low"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BONoLowInternalConsistency" level="WARNING" message="The boundary object has a low internal consistency." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:self.internalConsistency &lt;> bomi::HighLow::Low"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BONo(HighDetailLevelAndHighFrequencyChange)" level="WARNING" message="The boundary object has a high level of detail and a high frequency of change." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:not (self.levelOfDetail = bomi::HighLow::High and self.frequencyOfChange = bomi::HighLow::High)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BONo((Deprecated/RetiredLifecycleStage)AndHighFrequencyChange)" level="WARNING" message="The boundary object has a later lifecycle but a high frequency of change." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:not ((self.lifecycleStage = bomi::LifecycleStage::Deprecate or self.lifecycleStage = bomi::LifecycleStage::Retire) and self.frequencyOfChange = bomi::HighLow::High)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UsageNoLowFitforpurpose" level="WARNING" message="The usage association is not fit for purpose." targetClass="bomi::UsageAssociation">
          <audits auditExpression="aql:self.fitForPurpose &lt;> bomi::HighLow::Low"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UsageNo(HighCriticalityAndLowStability)" level="WARNING" message="The usage association has a high criticality but a low stability." targetClass="bomi::UsageAssociation">
          <audits auditExpression="aql:not (self.criticality = bomi::HighLow::High and self.stability = bomi::HighLow::Low)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BONo(NoGovernanceTeam)" level="WARNING" message="The boundary object has no governance team." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:not self.eContainer().boTeamGovernances.boundaryObjects->excludes(self)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BOAtLeastOneResponsibleRole" level="WARNING" message="The boundary object has no responsible role." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:self.eContainer().boRoleInteractions->select(i | i.boundaryObjects->includes(self) and i.responsible = bomi::UnsettableBoolean::True)->size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BOAtLeastOneUpdatingRole" level="WARNING" message="The boundary object has no updating role." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:self.eContainer().boRoleInteractions->select(i | i.boundaryObjects->includes(self) and i.updates = bomi::UnsettableBoolean::True)->size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BONoGoverningRoleInteractions" level="WARNING" message="For the governing roles of this boundary object, there should be an interaction between the roles and the boundary object." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:let referencingBOTeamGovernances = self.eContainer().boTeamGovernances->select(g | g.boundaryObjects->includes(self)) let referencingRoleInteractions = self.eContainer().boRoleInteractions->select(i | i.boundaryObjects->includes(self)) referencingRoleInteractions.roles->select(r | r.partOfGovernanceTeams->intersection(referencingBOTeamGovernances.governanceTeams) &lt;> null)->size() > 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="BONo(HighFrequencyOfChangeAndLowFrequencyOfCoordination)" level="WARNING" message="The boundary object has a high frequency of change but it's team governance a low frequency of coordination." targetClass="bomi::BoundaryObject">
          <audits auditExpression="aql:self.frequencyOfChange &lt;> bomi::HighLow::High or self.eContainer().boTeamGovernances->select(g | g.frequencyOfCoordination = bomi::HighLow::Low and g.boundaryObjects->includes(self))->size()=0"/>
        </ownedRules>
      </validationSet>
